{
  "name": "Caffeinated",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Casterlabs <support@casterlabs.co>",
  "build": {
    "icon": "assets/app_icon.icns",
    "productName": "Casterlabs Caffeinated",
    "files": [
      "__sapper__/export/**/*",
      "main.js"
    ],
    "appId": "casterlabs.caffeinated",
    "artifactName": "Caffeinated-${os}.${ext}",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "icon": "assets/app_icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "scripts": {
    "electron-dev-start": "wait-on http://localhost:3000 && electron .",
    "electron-dev": "concurrently \"sapper dev\" \"npm run electron-dev-start\"",
    "electron-package": "sapper export && electron-builder"
  },
  "dependencies": {
    "decompress-zip": "^0.3.3",
    "electron-dl": "^3.2.1",
    "electron-serve": "^1.0.0",
    "electron-store": "^8.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "compression": "^1.7.1",
    "concurrently": "^6.2.0",
    "electron": "^13.1.6",
    "electron-builder": "^22.8.0",
    "npm-run-all": "^4.1.5",
    "polka": "^0.5.2",
    "sapper": "^0.29.1",
    "sirv": "^1.0.0",
    "svelte": "^3.17.3",
    "svelte-loader": "^3.1.2",
    "wait-on": "^6.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-url": "^5.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0"
  }
}