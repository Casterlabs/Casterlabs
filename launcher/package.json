{
  "name": "Caffeinated",
  "version": "1.0.0",
  "author": "Casterlabs <support@casterlabs.co>",
  "build": {
    "icon": "assets/app_icon.icns",
    "productName": "Casterlabs Caffeinated",
    "files": [
      "build/**/*",
      "main.cjs"
    ],
    "appId": "casterlabs.caffeinated",
    "artifactName": "Caffeinated-${os}.${ext}",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "icon": "assets/app_icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "main": "main.cjs",
  "type": "module",
  "scripts": {
    "sveltekit-dev-start": "svelte-kit dev",
    "electron-dev-start": "wait-on http://localhost:3000 && electron .",
    "dev": "concurrently \"svelte-kit dev\" \"npm run electron-dev-start\"",
    "package": "svelte-kit build && electron-builder"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.17",
    "@sveltejs/kit": "^1.0.0-next.179",
    "concurrently": "^6.3.0",
    "electron": "^13.5.1",
    "electron-builder": "^22.8.0",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.34.0",
    "svelte-loader": "^3.1.2",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "decompress-zip": "^0.3.3",
    "electron-dl": "^3.2.1",
    "electron-store": "^8.0.0",
    "node-fetch": "^2.6.1",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1",
    "electron-serve": "^1.1.0",
    "svelte-kit": "^1.0.0"
  }
}
