{
  "name": "electron-sapper-rollup-template",
  "version": "1.0.0",
  "description": "A template project to get you started with using Sapper and Electron together.",
  "main": "main.js",
  "author": "e3ndr",
  "license": "MIT",
  "build": {
    "icon": "static/logo-512.png",
    "productName": "Electron + Sapper App (Rollup)",
    "files": [
      "__sapper__/export/**/*",
      "main.js"
    ]
  },
  "scripts": {
    "electron-dev-start": "wait-on http://localhost:3000 && electron .",
    "electron-dev": "concurrently \"sapper dev\" \"npm run electron-dev-start\"",
    "electron-package": "sapper export && electron-builder"
  },
  "dependencies": {
    "electron-serve": "^1.0.0"
  },
  "devDependencies": {
    "compression": "^1.7.1",
    "concurrently": "^6.2.0",
    "electron": "^13.1.6",
    "electron-builder": "^22.8.0",
    "npm-run-all": "^4.1.5",
    "polka": "^0.5.2",
    "sapper": "^0.29.1",
    "sirv": "^1.0.0",
    "svelte": "^3.17.3",
    "svelte-loader": "^3.1.2",
    "wait-on": "^6.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-url": "^5.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0"
  }
}