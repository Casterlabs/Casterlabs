{
  "name": "casterlabs-caffeinated",
  "version": "1.0.0",
  "author": "Casterlabs <support@casterlabs.co>",
  "build": {
    "icon": "assets/app_icon.icns",
    "productName": "Caffeinated Dummy Build",
    "files": [
      "build/**/*",
      "default-modules/**/*",
      "main.cjs",
      "createWindow.cjs"
    ],
    "appId": "casterlabs.caffeinated",
    "artifactName": "Casterlabs Caffeinated-${os}.${ext}",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "icon": "assets/app_icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "main": "main.cjs",
  "type": "module",
  "scripts": {
    "sveltekit-dev-start": "svelte-kit dev",
    "electron-dev-start": "wait-on http://localhost:3000 && electron .",
    "dev": "concurrently \"svelte-kit dev\" \"npm run electron-dev-start\"",
    "package": "svelte-kit build && electron-builder"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.17",
    "@sveltejs/kit": "^1.0.0-next.179",
    "concurrently": "^6.3.0",
    "electron": "^11.4.10",
    "electron-builder": "^22.8.0",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.34.0",
    "svelte-loader": "^3.1.2",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "electron-store": "^6.0.1",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1",
    "electron-serve": "^1.1.0",
    "svelte-kit": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "node-machine-id": "^1.1.12",
    "obs-websocket-js": "^4.0.3",
    "computer-name": "^0.1.0",
    "discord-rpc": "^4.0.1"
  }
}